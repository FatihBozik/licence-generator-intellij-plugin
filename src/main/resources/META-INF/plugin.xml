<idea-plugin>
    <id>io.github.fatihbozik.intellij-license-generator</id>
    <name>License Generator</name>
    <vendor email="bozikfatih@gmail.com" url="https://fatihbozik.github.io">Fatih Bozik</vendor>

    <description><![CDATA[
    Open Source License generator plugin for IntelliJ Idea
    ]]></description>

    <depends>com.intellij.modules.lang</depends>

    <idea-version since-build="221.0"/>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <action id="License.New" class="io.github.fatihbozik.licensegenerator.actions.NewLicenseFileGroupAction">
            <add-to-group group-id="NewGroup" anchor="last"/>
        </action>
    </actions>
</idea-plugin>
